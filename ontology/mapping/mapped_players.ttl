@prefix dbo: <http://dbpedia.org/ontology/> .
@prefix kg: <https://kg-football.vn/ontology#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

kg:Defender a owl:Class ;
    rdfs:label "Defender"@en,
        "Hậu vệ"@vi ;
    rdfs:subClassOf kg:Position ;
    owl:equivalentClass dbo:Defender .

kg:Forward a owl:Class ;
    rdfs:label "Forward"@en,
        "Tiền đạo"@vi ;
    rdfs:subClassOf kg:Position ;
    owl:equivalentClass dbo:Forward .

kg:Goalkeeper a owl:Class ;
    rdfs:label "Goalkeeper"@en,
        "Thủ môn"@vi ;
    rdfs:subClassOf kg:Position ;
    owl:equivalentClass dbo:Goalkeeper .

kg:LeftFooted a kg:Footedness ;
    rdfs:label "Left"@en,
        "Chân trái"@vi .

kg:Midfielder a owl:Class ;
    rdfs:label "Midfielder"@en,
        "Tiền vệ"@vi ;
    rdfs:subClassOf kg:Position ;
    owl:equivalentClass dbo:Midfielder .

kg:RightFooted a kg:Footedness ;
    rdfs:label "Right"@en,
        "Chân phải"@vi .

kg:birthPlace a owl:ObjectProperty ;
    rdfs:label "birth place"@en,
        "quê quán"@vi ;
    rdfs:domain kg:Player ;
    rdfs:range kg:City ;
    owl:equivalentProperty dbo:birthPlace .

kg:height a owl:DatatypeProperty ;
    rdfs:label "height (m)"@en,
        "chiều cao (m)"@vi ;
    rdfs:domain kg:Player ;
    rdfs:range xsd:decimal ;
    owl:equivalentProperty dbo:height .

kg:nationality a owl:ObjectProperty ;
    rdfs:label "nationality"@en,
        "quốc tịch"@vi ;
    rdfs:domain kg:Player ;
    rdfs:range kg:Country ;
    owl:equivalentProperty dbo:nationality .

kg:preferredFoot a owl:ObjectProperty ;
    rdfs:label "preferred foot"@en,
        "chân thuận"@vi ;
    rdfs:domain kg:Player ;
    rdfs:range kg:Footedness .

kg:primaryPosition a owl:ObjectProperty ;
    rdfs:label "primary position"@en,
        "vị trí sở trường"@vi ;
    rdfs:domain kg:Player ;
    rdfs:range kg:Position ;
    rdfs:subPropertyOf kg:playsPosition ;
    owl:equivalentProperty dbo:position .

kg:secondaryPosition a owl:ObjectProperty ;
    rdfs:label "secondary position"@en,
        "vị trí phụ"@vi ;
    rdfs:domain kg:Player ;
    rdfs:range kg:Position ;
    rdfs:subPropertyOf kg:playsPosition ;
    owl:equivalentProperty dbo:position .

kg:weight a owl:DatatypeProperty ;
    rdfs:label "weight (kg)"@en,
        "cân nặng (kg)"@vi ;
    rdfs:domain kg:Player ;
    rdfs:range xsd:decimal ;
    owl:equivalentProperty dbo:weight .

kg:playsPosition a owl:ObjectProperty ;
    rdfs:label "plays position"@en,
        "chơi ở vị trí"@vi ;
    rdfs:domain kg:Player ;
    rdfs:range kg:Position .

kg:Footedness a owl:Class ;
    rdfs:label "Footedness"@en,
        "Chân thuận"@vi .

kg:Player owl:equivalentClass dbo:SoccerPlayer .

