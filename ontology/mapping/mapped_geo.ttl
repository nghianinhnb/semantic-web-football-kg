@prefix geo: <http://www.opengis.net/ont/geosparql#> .
@prefix kg: <https://kg-football.vn/ontology#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix schema1: <http://schema.org/> .

kg:City a owl:Class ;
    rdfs:label "City"@en,
        "Thành phố"@vi ;
    rdfs:subClassOf kg:Place ;
    owl:equivalentClass <http://dbpedia.org/ontology/City> .

kg:Country a owl:Class ;
    rdfs:label "Country"@en,
        "Quốc gia"@vi ;
    rdfs:subClassOf kg:Place ;
    owl:equivalentClass <http://dbpedia.org/ontology/Country> .

kg:geometry a owl:ObjectProperty ;
    rdfs:label "geometry"@en,
        "hình học"@vi ;
    rdfs:domain kg:Place ;
    rdfs:range geo:Feature ;
    owl:equivalentProperty geo:hasGeometry .

kg:locatedIn a owl:ObjectProperty ;
    rdfs:label "located in"@en,
        "thuộc"@vi ;
    rdfs:domain kg:Place ;
    rdfs:range kg:Place ;
    owl:equivalentProperty <http://dbpedia.org/ontology/isPartOf> .

kg:wkt a owl:DatatypeProperty ;
    rdfs:label "WKT"@en,
        "WKT"@vi ;
    rdfs:domain kg:Place ;
    rdfs:range geo:wktLiteral ;
    owl:equivalentProperty geo:asWKT .

kg:Place a owl:Class ;
    rdfs:label "Place"@en,
        "Địa điểm"@vi ;
    rdfs:subClassOf schema1:Place ;
    owl:equivalentClass <http://dbpedia.org/ontology/Place> .

