@prefix kg: <https://kg-football.vn/ontology#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix schema1: <http://schema.org/> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

kg:NationalTeam a owl:Class ;
    rdfs:label "National Team"@en,
        "Đội tuyển quốc gia"@vi ;
    rdfs:subClassOf kg:Team ;
    owl:equivalentClass <http://dbpedia.org/ontology/NationalFootballTeam> .

kg:Referee a owl:Class ;
    rdfs:label "Referee"@en,
        "Trọng tài"@vi ;
    rdfs:subClassOf schema1:Person ;
    owl:equivalentClass <http://dbpedia.org/ontology/Referee> .

kg:capacity a owl:DatatypeProperty ;
    rdfs:label "capacity"@en,
        "sức chứa"@vi ;
    rdfs:domain kg:Stadium ;
    rdfs:range xsd:integer ;
    owl:equivalentProperty <http://dbpedia.org/ontology/capacity> .

kg:foundedDate a owl:DatatypeProperty ;
    rdfs:label "founded date"@en,
        "ngày thành lập"@vi ;
    rdfs:domain kg:Club ;
    rdfs:range xsd:date ;
    owl:equivalentProperty <http://dbpedia.org/ontology/foundingDate> .

kg:homeStadium a owl:ObjectProperty ;
    rdfs:label "home stadium"@en,
        "sân nhà"@vi ;
    rdfs:domain kg:Club ;
    rdfs:range kg:Stadium ;
    owl:equivalentProperty <http://dbpedia.org/ontology/ground> ;
    owl:inverseOf kg:isHomeOf .

kg:manages a owl:ObjectProperty ;
    rdfs:label "manages"@en,
        "dẫn dắt"@vi ;
    rdfs:domain kg:Coach ;
    rdfs:range kg:Team ;
    owl:equivalentProperty <http://dbpedia.org/ontology/manager> ;
    owl:inverseOf kg:hasCoach .

kg:owns a owl:ObjectProperty ;
    rdfs:label "owns"@en,
        "sở hữu"@vi ;
    rdfs:domain kg:Owner ;
    rdfs:range kg:Club ;
    owl:equivalentProperty <http://dbpedia.org/ontology/owner> ;
    owl:inverseOf kg:hasOwner .

kg:venue a owl:ObjectProperty ;
    rdfs:label "venue"@en,
        "địa điểm"@vi ;
    rdfs:range kg:Stadium ;
    owl:equivalentProperty <http://dbpedia.org/ontology/ground> .

kg:hasCoach a owl:ObjectProperty ;
    rdfs:label "has coach"@en,
        "có HLV"@vi ;
    rdfs:domain kg:Team ;
    rdfs:range kg:Coach ;
    owl:equivalentProperty <http://dbpedia.org/ontology/manager> .

kg:hasOwner a owl:ObjectProperty ;
    rdfs:label "has owner"@en,
        "có chủ sở hữu"@vi ;
    rdfs:domain kg:Club ;
    rdfs:range kg:Owner ;
    owl:equivalentProperty <http://dbpedia.org/ontology/owner> .

kg:isHomeOf a owl:ObjectProperty ;
    rdfs:label "is home of"@en,
        "là sân nhà của"@vi ;
    rdfs:domain kg:Stadium ;
    rdfs:range kg:Club ;
    owl:equivalentProperty <http://dbpedia.org/ontology/ground> .

kg:Coach a owl:Class ;
    rdfs:label "Coach"@en,
        "Huấn luyện viên"@vi ;
    rdfs:subClassOf schema1:Person ;
    owl:equivalentClass <http://dbpedia.org/ontology/SoccerCoach> .

kg:Owner a owl:Class ;
    rdfs:label "Owner"@en,
        "Chủ sở hữu"@vi ;
    rdfs:subClassOf schema1:Person ;
    owl:equivalentClass <http://dbpedia.org/ontology/Person> .

kg:Stadium a owl:Class ;
    rdfs:label "Stadium"@en,
        "Sân vận động"@vi ;
    rdfs:subClassOf schema1:SportsVenue ;
    owl:equivalentClass <http://dbpedia.org/ontology/Stadium> .

kg:Team a owl:Class ;
    rdfs:label "Team"@en,
        "Đội bóng"@vi ;
    rdfs:subClassOf schema1:SportsTeam ;
    owl:equivalentClass <http://dbpedia.org/ontology/SportsTeam> .

kg:Club rdfs:subClassOf kg:Team ;
    owl:equivalentClass <http://dbpedia.org/ontology/FootballClub> .

