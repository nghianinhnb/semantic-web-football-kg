@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix schema: <http://schema.org/> .
@prefix geo: <http://www.opengis.net/ont/geosparql#> .
@prefix kg: <https://kg-football.vn/ontology#> .
@prefix res: <https://kg-football.vn/resource/> .

# Geographic entities using standard vocabularies
kg:Place a owl:Class ;
  rdfs:label "Place"@en , "Địa điểm"@vi ;
  rdfs:comment "Geographic place"@en ;
  rdfs:subClassOf schema:Place ,
                  geo:Feature .

kg:City a owl:Class ;
  rdfs:label "City"@en , "Thành phố"@vi ;
  rdfs:comment "City"@en ;
  rdfs:subClassOf kg:Place ,
                  schema:City .

kg:Country a owl:Class ;
  rdfs:label "Country"@en , "Quốc gia"@vi ;
  rdfs:comment "Country"@en ;
  rdfs:subClassOf kg:Place ,
                  schema:Country .

# Geographic relationships using standard vocabularies
kg:locatedIn a owl:ObjectProperty ;
  rdfs:label "located in"@en , "thuộc"@vi ;
  rdfs:comment "Place is located within another place"@en ;
  rdfs:domain kg:Place ;
  rdfs:range kg:Place ;
  rdfs:subPropertyOf schema:containedInPlace .
