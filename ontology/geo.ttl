@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix schema: <http://schema.org/> .
@prefix geo: <http://www.opengis.net/ont/geosparql#> .
@prefix kg: <https://kg-football.vn/ontology#> .
@prefix res: <https://kg-football.vn/resource/> .

kg:Place a owl:Class ;
  rdfs:label "Place"@en , "Địa điểm"@vi ;
  rdfs:subClassOf schema:Place .

kg:City a owl:Class ;
  rdfs:label "City"@en , "Thành phố"@vi ;
  rdfs:subClassOf kg:Place .

kg:Country a owl:Class ;
  rdfs:label "Country"@en , "Quốc gia"@vi ;
  rdfs:subClassOf kg:Place .

kg:locatedIn a owl:ObjectProperty ;
  rdfs:label "located in"@en , "thuộc"@vi ;
  rdfs:domain kg:Place ;
  rdfs:range kg:Place .

kg:geometry a owl:ObjectProperty ;
  rdfs:label "geometry"@en , "hình học"@vi ;
  rdfs:domain kg:Place ;
  rdfs:range geo:Feature .

kg:wkt a owl:DatatypeProperty ;
  rdfs:label "WKT"@en , "WKT"@vi ;
  rdfs:domain kg:Place ;
  rdfs:range geo:wktLiteral .


