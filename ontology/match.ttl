@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix schema: <http://schema.org/> .
@prefix kg: <https://kg-football.vn/ontology#> .
@prefix res: <https://kg-football.vn/resource/> .

# Match properties using standard vocabularies
kg:inCompetition a owl:ObjectProperty ;
  rdfs:label "in competition"@en , "thuộc giải"@vi ;
  rdfs:comment "Match is part of a competition"@en ;
  rdfs:domain kg:Match ;
  rdfs:range kg:Competition ;
  rdfs:subPropertyOf schema:partOf .

kg:inSeason a owl:ObjectProperty ;
  rdfs:label "in season"@en , "thuộc mùa giải"@vi ;
  rdfs:comment "Match is part of a season"@en ;
  rdfs:domain kg:Match ;
  rdfs:range kg:Season ;
  rdfs:subPropertyOf schema:partOf .
