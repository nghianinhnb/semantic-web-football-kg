@prefix kg: <https://kg-football.vn/ontology#> .
@prefix res: <https://kg-football.vn/resource/> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .

res:club/Sông Bé a kg:Club ;
    kg:teamName "Đội bóng đá Sông Bé" ;
    kg:teamName "Đội bóng đá Cao su Bình Long" ;
    kg:foundedDate "1974-01-01"^^xsd:date ;
    kg:hasCoach res:coach/Đỗ Thới Vinh ;
    kg:hasCoach res:coach/Nguyễn Kim Phụng ;
    kg:participatesIn res:season/1995 ;
    kg:locatedIn res:city/Thủ Dầu Một ;
    kg:locatedIn res:country/Việt Nam .

res:club/Sông Bé a kg:disbandedDate "1997-01-01"^^xsd:date .

res:city/Thủ Dầu Một a kg:City ;
    rdfs:label "Thủ Dầu Một" .

res:country/Việt Nam a kg:Country ;
    rdfs:label "Việt Nam" .

res:coach/Đỗ Thới Vinh a kg:Coach ;
    kg:manages res:club/Sông Bé .

res:coach/Nguyễn Kim Phụng a kg:Coach ;
    kg:manages res:club/Sông Bé .

res:season/1995 a kg:Season ;
    kg:seasonOf res:competition/Giải vô địch bóng đá Việt Nam 1995 .

res:competition/Giải vô địch bóng đá Việt Nam 1995 a kg:Competition ;
    kg:competitionName "Giải vô địch bóng đá Việt Nam 1995" .

res:club/Sông Bé a kg:hasAchievement "Cúp Quốc gia Việt Nam 1994" .

res:club/Sông Bé a kg:note "Xuống hạng (bỏ cuộc)" .

res:club/Sông Bé a kg:rebrandedAs res:club/Cao su Bình Long .

res:club/Cao su Bình Long a kg:Club ;
    kg:teamName "Đội bóng đá Cao su Bình Long" ;
    kg:foundedDate "1996-01-01"^^xsd:date ;
    kg:locatedIn res:city/Bình Long ;
    kg:locatedIn res:country/Việt Nam ;
    kg:disbandedDate "1997-01-01"^^xsd:date .

res:city/Bình Long a kg:City ;
    rdfs:label "Bình Long" .

res:club/Cao su Bình Long a kg:participatesIn res:season/1996 .

res:season/1996 a kg:Season ;
    kg:seasonOf res:competition/Giải bóng đá Các đội mạnh Toàn quốc lần thứ V .

res:competition/Giải bóng đá Các đội mạnh Toàn quốc lần thứ V a kg:Competition ;
    kg:competitionName "Giải bóng đá Các đội mạnh Toàn quốc lần thứ V" .
