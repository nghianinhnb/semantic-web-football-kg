@prefix kg: <https://kg-football.vn/ontology#> .
@prefix res: <https://kg-football.vn/resource/> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .

res:position/Defender a kg:Defender ;
rdfs:label "Hậu vệ" .

res:position/Midfielder a kg:Midfielder ;
rdfs:label "Tiền vệ" .

res:position/Forward a kg:Forward ;
rdfs:label "Tiền đạo" .

res:position/Goalkeeper a kg:Goalkeeper ;
rdfs:label "Thủ môn" .

res:formation/4-5-1 a kg:Formation ;
kg:hasDefenders "4"^^xsd:positiveInteger ;
kg:hasMidfielders "5"^^xsd:positiveInteger ;
kg:hasForwards "1"^^xsd:positiveInteger ;
rdfs:label "4–5–1".

res:formation/4-4-2 a kg:Formation ;
kg:hasDefenders "4"^^xsd:positiveInteger ;
kg:hasMidfielders "4"^^xsd:positiveInteger ;
kg:hasForwards "2"^^xsd:positiveInteger ;
rdfs:label "4–4–2".

res:formation/4-2-1-3 a kg:Formation ;
kg:hasDefenders "4"^^xsd:positiveInteger ;
kg:hasMidfielders "2"^^xsd:positiveInteger ;
kg:hasMidfielders "1"^^xsd:positiveInteger ;
kg:hasForwards "3"^^xsd:positiveInteger ;
rdfs:label "4–2–1–3".

res:formation/4-1-2-1-2 a kg:Formation ;
kg:hasDefenders "4"^^xsd:positiveInteger ;
kg:hasMidfielders "1"^^xsd:positiveInteger ;
kg:hasMidfielders "2"^^xsd:positiveInteger ;
kg:hasMidfielders "1"^^xsd:positiveInteger ;
kg:hasForwards "2"^^xsd:positiveInteger ;
rdfs:label "4–1–2–1–2".

res:formation/4-4-2-diamond a kg:Formation ;
rdfs:label "4–4–2 hình thoi" ;
kg:relatedFormation res:formation/4-4-2 .

res:football/formationDescription a kg:Description ;
kg:description """Trong bóng đá, sơ đồ chiến thuật, sơ đồ đội hình hay đội hình mô tả vị trí các cầu thủ trong một đội nói chung trên sân..."""@vi .